+            m_botmap[pos]->m_entry = m_entry;
+            m_botmap[pos]->m_race = m_race;
+            m_botmap[pos]->m_class = m_class;
+
+            for (uint8 i = 0; i != 18; ++i)
+                m_botmap[pos]->equips[i] = equips[i];
+
+            break;
+        }
+    }
