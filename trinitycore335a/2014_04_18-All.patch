+        void ReduceCD(uint32 diff)
+        {
+            CommonTimers(diff);
+            SpellTimers(diff);
+
+            if (Trap_cd > diff)                     Trap_cd -= diff;
+
+            if (ScorpidSting_Timer > diff)          ScorpidSting_Timer -= diff;
+            if (Aspect_Timer > diff)                Aspect_Timer -= diff;
